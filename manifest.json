{
    "manifest_version": 3,
    "name": "Ultimate Tab Manager",
    "version": "1.0.0",
    "description": "Comprehensive tab management and export tool",
    "permissions": [
        "tabs",
        "storage",
        "downloads",
        "bookmarks",
        "activeTab",
        "favicon"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}